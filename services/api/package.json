{
  "devDependencies": {
    "jest": "^30.2.0",
    "supertest": "^7.2.2"
  }
}

{
  "name": "enterprise-api",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": ["/node_modules/"],
    "testMatch": ["**/__tests__/**/*.js", "**/?(*.)+(spec|test).js"]
  }
}